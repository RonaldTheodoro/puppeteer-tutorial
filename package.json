{
  "name": "puppeteer-tutorial",
  "version": "1.0.0",
  "description": "Automated software testing with Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests",
    "test:example": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/example.test.js",
    "test:device": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/device.test.js",
    "test:e2e": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/e2e/*.test.js",
    "test:e2e:feedback": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/e2e/feedback.test.js",
    "test:e2e:login": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/e2e/login.test.js",
    "test:e2e:payment": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/e2e/payment.test.js",
    "test:e2e:currency-exchange": "./node_modules/mocha/bin/mocha --timeout 30000 ./tests/e2e/currency-exchange.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "puppeteer": "^7.0.1"
  }
}
